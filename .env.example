# LangGraph Multi-Agent Platform Environment Configuration

# Core API Keys (REQUIRED)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=langgraph-multi-agent-platform
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Database Configuration
# Supabase (Primary Database)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Hasura GraphQL (API Layer)
HASURA_GRAPHQL_ENDPOINT=your_hasura_endpoint_here
HASURA_ADMIN_SECRET=your_hasura_admin_secret_here

# Database URLs
DATABASE_URL=postgresql://user:password@localhost:5432/langgraph_agents
REDIS_URL=redis://localhost:6379/0

# Vercel Deployment
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here

# Cloudflare Workers
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Cloudinary (File Storage)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name_here
CLOUDINARY_API_KEY=your_cloudinary_api_key_here
CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here

# Clerk Authentication
CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username_here

# n8n Automation
N8N_WEBHOOK_URL=your_n8n_webhook_url_here
N8N_API_KEY=your_n8n_api_key_here

# Agent Configuration
DEFAULT_MODEL=claude-3-sonnet-20240229
TEMPERATURE=0.7
MAX_TOKENS=4000
MAX_RETRIES=3
REQUEST_TIMEOUT=60

# Project Generation
OUTPUT_DIR=./output
GENERATED_PROJECTS_DIR=./generated_projects
MAX_CONCURRENT_AGENTS=5
AGENT_MEMORY_SIZE=10000

# Monitoring and Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/agents.log
ENABLE_DETAILED_LOGGING=true

# Security
SECRET_KEY=your_secret_key_here
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8501
CORS_ENABLED=true

# Development Settings
DEBUG_MODE=false
ENABLE_AGENT_TRACING=true
CACHE_ENABLED=true
CACHE_TTL=3600

# Testing
TEST_MODE=false
MOCK_EXTERNAL_APIS=false